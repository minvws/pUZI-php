{
  "name": "minvws/puzi-php",
  "description": "PHP library to read data from an UZI pass.",
  "keywords": [
    "security",
    "crypto",
    "cryptography",
    "signature",
    "x509",
    "x.509",
    "asn1",
    "asn.1",
    "UZI"
  ],
  "type": "library",
  "license": "EUPL-1.2",
  "authors": [
    {
      "name": "Anne Jan Brouwer",
      "email": "brouwer@annejan.com"
    },
    {
      "name": "Nick ten Cate",
      "email": "nick@nicktencate.com"
    },
    {
      "name": "Mendel Mobach",
      "email": "mendel@mobach.nl"
    },
    {
      "name": "Joshua Thijssen",
      "email": "jthijssen@noxlogic.nl"
    }
  ],
  "require": {
    "php": ">=7.3",
    "phpseclib/phpseclib": "^3.0.5",
    "psr/http-message": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.4",
    "phpstan/phpstan": "^1.8",
    "squizlabs/php_codesniffer": "^3.7",
    "symfony/http-foundation": "^6"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\PUZI\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\PUZI\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-openssl": "Install the OpenSSL extension in order to speed up a wide variety of cryptographic operations.",
    "ext-gmp": "Install the GMP (GNU Multiple Precision) extension in order to speed up arbitrary precision integer arithmetic operations."
  },
  "scripts": {
    "test": [
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm",
        "vendor/bin/phpcs"
    ]
  }
}
